    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Student Grade Evaluator</title>
</head>
<body>
    <h1>Student Grade Evaluator</h1>
    <script>
        let marks = 60;

        if(marks >= 75){
            document.write("Grade A");
        }
        else if (marks >= 60){
            document.write("Grade B");

        }
        else if (marks >= 40)
        {
        document.write("Grade B");
    }
    else
    {
        document.write("Fail");
    }

    </script>
</body>
</html>
